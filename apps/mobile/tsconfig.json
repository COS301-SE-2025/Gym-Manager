{
  "compilerOptions": {
    "jsx": "react-native",
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "target": "es6",
    "module": "esnext",
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
